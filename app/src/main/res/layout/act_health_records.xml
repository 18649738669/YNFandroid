<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical">
    <include layout="@layout/tb_common"/>

    <LinearLayout
        android:id="@+id/health_records_ll_health_report"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/health_records"
            android:layout_marginLeft="@dimen/text_mid"
            android:layout_marginRight="@dimen/text_mid"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="@string/health_records_ll_health_report"
            android:textColor="@color/common_black"
            android:textSize="@dimen/text_mid"/>
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/right"
            android:layout_marginLeft="@dimen/text_mid"
            android:layout_marginRight="@dimen/text_mid"/>
    </LinearLayout>
    <include layout="@layout/view_interval_gray"/>

    <LinearLayout
        android:id="@+id/health_records_ll_medical_report"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/health_records"
            android:layout_marginLeft="@dimen/text_mid"
            android:layout_marginRight="@dimen/text_mid"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="@string/health_records_ll_medical_report"
            android:textColor="@color/common_black"
            android:textSize="@dimen/text_mid"/>
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/right"
            android:layout_marginLeft="@dimen/text_mid"
            android:layout_marginRight="@dimen/text_mid"/>
    </LinearLayout>
    <include layout="@layout/view_interval_gray"/>


</LinearLayout>
